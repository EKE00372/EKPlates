			<!-- 施法條 -->
			<StatusBar name="$parentCastBar" parentKey="castBar" frameStrata="HIGH" inherits="EKPlatesNormalCastBarTemplate">
				<Size>
					<AbsDimension x = "90" y = "8"/>
				</Size>	
				
				<Layers>
					<Layer level="OVERLAY">
						<!-- 圖示 -->
						<Texture parentKey="Icon">
							<Anchors>
								<Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMLEFT">
									<Offset x="-4" y="-12"/>
								</Anchor>
							</Anchors>
							<Size>
								<AbsDimension x = "20" y = "20"/>
							</Size>
							<TexCoords left="0.08" right="0.92" top="0.08" bottom="0.92"/>
						</Texture>
					</Layer>
				</Layers>
			</StatusBar>
			
			<!-- 團隊標記 -->
			<Frame name="$parentRaidTargetFrame" parentKey="RaidTargetFrame" inherits="EKPlatesRaidTargetTemplate">
				<Anchors>
					<Anchor point="RIGHT" relativeTo="$parentName" relativePoint="LEFT">
						<Offset x="0" y="0"/>
					</Anchor>
				</Anchors>
			</Frame>
			
			<!-- 光環 -->
			<Frame name="$parentIcons" parentKey="icons" inherits="EKPlatesIconHolderTemplate">
				<Anchors>
					<Anchor point="BOTTOM" relativeTo="$parentHealthperc" relativePoint="TOP">
						<Offset x="0" y="0"/>
					</Anchor>
				</Anchors>
			</Frame>
		</Frames>
		<Scripts>
			<OnLoad>

			</OnLoad>
		</Scripts>
	</Button>
	
	<!-- 條形模式 -->
	<Button name="EKPlatesBarStyleNameplateTemplate" setAllPoints="true" virtual="true">
		<Layers>
			<Layer level="OVERLAY">
				<Texture name="$parenthltarget" parentKey="hltarget" file="Interface\AddOns\EKPlates\media\NeonRedArrow" hidden="true">
					<Size>
						<AbsDimension x = "50" y = "50"/>
					</Size>
				</Texture>
			</Layer>
			
			<Layer level="ARTWORK">
				<!-- 名字 -->
				<FontString name="$parentName" parentKey="name" inherits="EKPlates_BarFont" justifyH="CENTER">
					<Anchors>
						<Anchor point="LEFT">
							<Offset x="5" y="2"/>
						</Anchor>
						
						<Anchor point="RIGHT">
							<Offset x="-5" y="2"/>
						</Anchor>
					</Anchors>
					
					<Color r="1" g="1" b="1"/>
					
					<Shadow>
						<Offset x="0" y="0" />
					</Shadow>
				</FontString>
			</Layer>
			
			<Layer level="BACKGROUND">
				<!-- 焦點 -->
				<Texture name="$parenthlfocus" parentKey="hlfocus" file="Interface\AddOns\EKPlates\media\hlglow" alphaMode="ADD" hidden="true">
					<Size>
						<AbsDimension x = "20" y = "30"/>
					</Size>
					<Color r=".3" g="1" b=".3"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" relativePoint="LEFT">
							<Offset x="-16" y="0"/>
						</Anchor>
						<Anchor point="BOTTOMRIGHT" relativePoint="RIGHT">
							<Offset x="16" y="0"/>
						</Anchor>
					</Anchors>
					<TexCoords left="0" right="1" top="1" bottom="0"/>
				</Texture>
				<!-- 指向 -->
				<Texture name="$parenthlmo" parentKey="hlmo" file="Interface\AddOns\EKPlates\media\hlglow" alphaMode="ADD" hidden="true">
					<Size>
						<AbsDimension x = "20" y = "30"/>
					</Size>
					<Color r="1" g="1" b="0"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" relativePoint="LEFT">
							<Offset x="-16" y="0"/>
						</Anchor>
						<Anchor point="BOTTOMRIGHT" relativePoint="RIGHT">
							<Offset x="16" y="0"/>
						</Anchor>
					</Anchors>
					<TexCoords left="0" right="1" top="1" bottom="0"/>
				</Texture>
			</Layer>
		</Layers>
		
		<Frames>
			<!-- 血量條 -->
			<StatusBar name="$parentHealthBar" parentKey="healthBar" frameStrata="HIGH" inherits="EKPlatesHealthBarTemplate">
				<Anchors>
					<Anchor point="LEFT">
						<Offset x="0" y="0"/>
					</Anchor>
					<Anchor point="RIGHT">
						<Offset x="0" y="0"/>
					</Anchor>
				</Anchors>
			</StatusBar>
			
			<!-- 能量條 -->
			<StatusBar name="$parentPowerBar" parentKey="powerBar" frameStrata="HIGH" inherits="EKPlatesPowerBarTemplate">
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativeTo="$parentHealthBar" relativePoint="TOPLEFT">
						<Offset x="0" y="0"/>
					</Anchor>
					<Anchor point="BOTTOMRIGHT" relativeTo="$parentHealthBar" relativePoint="TOPRIGHT">
						<Offset x="0" y="0"/>
					</Anchor>	
				</Anchors>
			</StatusBar>
			
			<!-- 施法條 -->
			<StatusBar name="$parentCastBar" parentKey="castBar" inherits="EKPlatesNormalCastBarTemplate"/>
			
			<!-- 團隊標記 -->
			<Frame name="$parentRaidTargetFrame" parentKey="RaidTargetFrame" inherits="EKPlatesRaidTargetTemplate">
				<Anchors>
					<Anchor point="RIGHT" relativeTo="$parentName" relativePoint="LEFT">
						<Offset x="0" y="0"/>
					</Anchor>
				</Anchors>
			</Frame>
			
			<!-- 光環 -->
			<Frame name="$parentIcons" parentKey="icons" inherits="EKPlatesIconHolderTemplate">
				<Anchors>
					<Anchor point="BOTTOM" relativePoint="TOP">
						<Offset x="0" y="0"/>
					</Anchor>
				</Anchors>
			</Frame>
		</Frames>
		
		<Scripts>
			<OnLoad>
				self.name:SetIndentedWordWrap(false)
				self.name:SetWordWrap(false)
			</OnLoad>
		</Scripts>
	</Button>
